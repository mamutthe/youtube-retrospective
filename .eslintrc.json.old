{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["/workspaces/yt-history-stats/history-analyzer/tsconfig.json"]
    },
    "plugins": [
        "react"
    ],
    "rules": {"@typescript-eslint/member-delimiter-style": "off",
    "semi": "off",
    "@typescript-eslint/semi": "off",
    "space-before-function-paren": "off",
    "@typescript-eslint/space-before-function-paren": "off",
    "@typescript-eslint/restrict-plus-operands": "off",
    "no-trailing-spaces": "off"
    }
}
